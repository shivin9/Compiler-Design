%Test Case 1
%Following function computes the sum of user defined real numbers
%The variable d3 maintains the sum of values
_sumN  input parameter list [int d5cc34, real d4, int d5cc]
output parameter list[real d3];
	type int :d3;
	d3 <--- d3 + (d5 + (b3 + c3) * 108);
	read(c3bd);
	return [d3];
end

_sumM  input parameter list [real d2cc34]
output parameter list[int d5];
	d5 <--- d5 + 128;
	read(c3bb);
	return [d5];
end

_main
	type record #marks : d4;
	type int :b3: global;
	type real :c4bd56;	

	while(b5<=d5cb34567)
		[d4] <--- call _readMarks with parameters [b5];
		b5c6 <--- b5c6 + d4; 
		% above displays the sum of records
		b5 <--- b5 +1;
	endwhile

	if((~(c4bbb == 0)) &&& (c4bbb > 78.56)) 
	then
		c6<--- d4cbcd5677/c4bbb;
	else
		write(c4bbb);
	endif


	b3 <--- 7;
	[c4bd56]<--- call _sumN with parameters [b3];
	write(c4bd56);
	return;
end
$
